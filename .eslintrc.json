{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["typescript", "plugin:promise/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint",
    "promise"
  ],
  "rules": {
    "no-extra-semi": "off"
  }
}